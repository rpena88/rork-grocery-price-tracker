{
  "buildCommand": "npm run build:production",
  "outputDirectory": "dist",
  "devCommand": "npm run web",
  "installCommand": "npm install",
  "framework": null,
  "functions": {
    "api/trpc/[trpc].ts": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/trpc/(.*)",
      "destination": "/api/trpc/[trpc]"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}